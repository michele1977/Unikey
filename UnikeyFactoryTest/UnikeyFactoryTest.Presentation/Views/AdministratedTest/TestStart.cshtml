@model UnikeyFactoryTest.Presentation.Models.AdministratedTestModel
<div class="container startContainer">
    <div class="myForm">
        <h3>Assessment</h3>
        <hr />
        <form action="/AdministratedTest/BeginTest" method="post">
            <input type="hidden" name="URL" value="@Model.URL" />
            <label class="myLabel">Name: </label>
            <input type="text" name="Name" class="myInput" placeholder="Your name..." />
            <br />
            <label class="myLabel">Surname:</label>
            <input type="text" name="Surname" class="myInput" placeholder="Your surname..." />
            <hr />
            <div class="buttonDiv">
                <button class="btn btn-success myButton">Begin</button>
            </div>
        </form>
    </div>
</div>