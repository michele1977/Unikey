@model UnikeyFactoryTest.Presentation.Models.DTO.TestDto

<tr>
    <td>
        @Model.Id
    </td>
    <td>
        @Model.Title
    </td>
    <td>
        @Model.Date
    </td>
    <td>
        @Model.URL
    </td>
    <td>
        @Model.ClosedTestNumber
    </td>
    <td>
    </td>
    <td>
        <div>
            @Html.ActionLink(
                linkText: "Content",
                actionName: "TestContent",
                controllerName: "Test",
                routeValues: new { @Model.Id, @Model.PageNumber, @Model.PageSize },
                htmlAttributes: new { @class = "btn btn-info" }) |
            <button class="btn btn-info" onclick="deleteConfirmation(@Model.Id, @Model.PageNumber, @Model.PageSize)">Delete</button>
            <input type="hidden" name="Id" value="@Model.Id" />
            <button class="btn btn-info" onclick="getDetailsTablePartial(@Model.Id)">
                Details
            </button>
            <button type="button" onclick="openModal(@Model.Id)" class="btn btn-info" id="openButton">Send</button>


        </div>
    </td>

</tr>


